{
	"extends": [
		"tslint:latest",
		"tslint-plugin-prettier",
		"tslint-config-prettier"
	],
	"defaultSeverity": "error",
	"rules": {
		"object-literal-sort-keys": false,
		"forin": false,
		"interface-name": false,
		"no-console": false,
		"one-variable-per-declaration": false,
		"variable-name": false,
		"array-type": false,
		"class-name": false
	}
}